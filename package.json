{
  "name": "zion-parser",
  "version": "0.0.3",
  "description": "A parser/Lexer for COBOL creating an AST",
  "main": "./dist/src/zion-parser.js",
  "types": "./dist/src/zion-parser.d.js",
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "pretest": "npm run build",
    "test-only": "jest --config jestconfig.json",
    "test": "jest --config jestconfig.json",
    "mocktest": "npm run build && jest --config jestconfig.json -t mock",
    "masstest": "npm run build && jest --config jestconfig.json -t mass",
    "clean": "shx rm -rf node_modules\\ dist\\ docs\\",
    "format": "prettier --write \"dev\\**\\*.ts\" \"dev\\**\\*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A dev",
    "postversion": "git push && git push --tags",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./dev/src/",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "parser",
    "lexer",
    "COBOL",
    "AST"
  ],
  "author": "Jakob Wagner",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^7.0.5",
    "@types/jest": "^23.3.14",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakobwgnr/zion-parser.git"
  },
  "bugs": {
    "url": "https://github.com/jakobwgnr/zion-parser/issues"
  },
  "homepage": "https://github.com/jakobwgnr/zion-parser.git#readme",
  "dependencies": {
    "codecov": "^3.2.0",
    "jest": "^23.0.0"
  }
}