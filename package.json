{
  "name": "zion-parser",
  "version": "0.0.1",
  "description": "A parser/Lexer for COBOL creating an AST",
  "main": "",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "parser",
    "lexer",
    "COBOL",
    "AST"
  ],
  "author": "Jakob Wagner",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.5",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakobwgnr/zion-parser.git"
  },
  "bugs": {
    "url": "https://github.com/jakobwgnr/zion-parser/issues"
  },
  "homepage": "https://github.com/jakobwgnr/zion-parser.git#readme"
}